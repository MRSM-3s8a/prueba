{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mine-costs-calculator-v2": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mine-costs-calculator-v2",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/vendors/general/animate.css/animate.css",
              "src/assets/vendors/general/morris.js/morris.css",
              "src/assets/vendors/custom/vendors/line-awesome/css/line-awesome.css",
              "src/assets/vendors/custom/vendors/flaticon/flaticon.css",
              "src/assets/vendors/custom/vendors/flaticon2/flaticon.css",
              "src/assets/vendors/general/nouislider/distribute/nouislider.css",
              "src/assets/vendors/general/@fortawesome/fontawesome-free/css/all.min.css",
              "src/assets/css/demo12/style.bundle.css",
              "src/assets/css/demo12/custom.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/vendors/general/jquery/dist/jquery.js",
              "src/assets/vendors/general/popper.js/dist/umd/popper.js",
              "src/assets/vendors/general/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/vendors/general/js-cookie/src/js.cookie.js",
              "src/assets/vendors/general/moment/min/moment.min.js",
              "src/assets/vendors/general/perfect-scrollbar/dist/perfect-scrollbar.js",
              "src/assets/vendors/general/sticky-js/dist/sticky.min.js",
              "src/assets/vendors/general/owl.carousel/dist/owl.carousel.js",
              "src/assets/vendors/general/morris.js/morris.js",
              "src/assets/vendors/general/jquery.repeater/src/lib.js",
              "src/assets/vendors/general/jquery.repeater/src/repeater.js",
              "src/assets/vendors/general/mdb.min.js",
              "src/assets/js/demo12/scripts.bundle.js",
              "src/assets/js/demo12/pages/dashboard.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mine-costs-calculator-v2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mine-costs-calculator-v2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mine-costs-calculator-v2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "mine-costs-calculator-v2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mine-costs-calculator-v2:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "mine-costs-calculator-v2"
}
